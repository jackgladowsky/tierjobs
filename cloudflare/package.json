{
  "name": "tierjobs-api",
  "version": "1.0.0",
  "description": "TierJobs API - AI-powered job board for elite tech companies",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "db:create": "wrangler d1 create tierjobs",
    "db:migrate": "wrangler d1 execute tierjobs --local --file=./schema.sql",
    "db:migrate:prod": "wrangler d1 execute tierjobs --remote --file=./schema.sql",
    "db:seed": "node scripts/seed.js",
    "tail": "wrangler tail"
  },
  "dependencies": {
    "hono": "^4.4.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240620.0",
    "typescript": "^5.4.5",
    "wrangler": "^4.66.0"
  }
}
